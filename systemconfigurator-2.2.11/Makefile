# --- MakeMaker manifypods section:

POD2MAN_EXE = $(PERLRUN) "-MExtUtils::Command::MM" -e pod2man "--"
POD2MAN = $(POD2MAN_EXE)


manifypods : pure_all  \
	bin/systemconfigurator
	$(NOECHO) $(POD2MAN) --section=$(MAN1EXT) --perm_rw=$(PERM_RW) \
	  bin/systemconfigurator $(INST_MAN1DIR)/systemconfigurator.$(MAN1EXT) 




# --- MakeMaker processPL section:


# --- MakeMaker installbin section:

EXE_FILES = bin/systemconfigurator bin/scconf-tool bin/scconf-bootinfo

pure_all :: $(INST_SCRIPT)/scconf-tool $(INST_SCRIPT)/scconf-bootinfo $(INST_SCRIPT)/systemconfigurator
	$(NOECHO) $(NOOP)

realclean ::
	$(RM_F) \
	  $(INST_SCRIPT)/scconf-tool $(INST_SCRIPT)/scconf-bootinfo \
	  $(INST_SCRIPT)/systemconfigurator 

$(INST_SCRIPT)/scconf-tool : bin/scconf-tool $(FIRST_MAKEFILE) $(INST_SCRIPT)$(DFSEP).exists $(INST_BIN)$(DFSEP).exists
	$(NOECHO) $(RM_F) $(INST_SCRIPT)/scconf-tool
	$(CP) bin/scconf-tool $(INST_SCRIPT)/scconf-tool
	$(FIXIN) $(INST_SCRIPT)/scconf-tool
	-$(NOECHO) $(CHMOD) $(PERM_RWX) $(INST_SCRIPT)/scconf-tool

$(INST_SCRIPT)/scconf-bootinfo : bin/scconf-bootinfo $(FIRST_MAKEFILE) $(INST_SCRIPT)$(DFSEP).exists $(INST_BIN)$(DFSEP).exists
	$(NOECHO) $(RM_F) $(INST_SCRIPT)/scconf-bootinfo
	$(CP) bin/scconf-bootinfo $(INST_SCRIPT)/scconf-bootinfo
	$(FIXIN) $(INST_SCRIPT)/scconf-bootinfo
	-$(NOECHO) $(CHMOD) $(PERM_RWX) $(INST_SCRIPT)/scconf-bootinfo

$(INST_SCRIPT)/systemconfigurator : bin/systemconfigurator $(FIRST_MAKEFILE) $(INST_SCRIPT)$(DFSEP).exists $(INST_BIN)$(DFSEP).exists
	$(NOECHO) $(RM_F) $(INST_SCRIPT)/systemconfigurator
	$(CP) bin/systemconfigurator $(INST_SCRIPT)/systemconfigurator
	$(FIXIN) $(INST_SCRIPT)/systemconfigurator
	-$(NOECHO) $(CHMOD) $(PERM_RWX) $(INST_SCRIPT)/systemconfigurator



# --- MakeMaker subdirs section:

