# --- MakeMaker selfdocument section:


# --- MakeMaker postamble section:

POD2MAN_EXE := $(POD2MAN_EXE) "--center=System Configurator" "--release=v2.2.11"
POD2MAN5_EXE = $(POD2MAN_EXE) "--section=5"

deb_prep :: manifest
	perl -pi.bak -e 's/^VERSION.*/VERSION = 2.2.11/' debian/rules

deb :: deb_prep dist
	mkdir -p /tmp/scdeb
	mv systemconfigurator-2.2.11.tar.gz /tmp/scdeb && cd /tmp/scdeb && tar -xvzf systemconfigurator-2.2.11.tar.gz && cd systemconfigurator-2.2.11 && dpkg-buildpackage && rm -rf /tmp/scdeb/systemconfigurator-2.2.11
	echo "Debian Packages have been built in /tmp/scdeb"

rpm_prep ::
	cp -a ./sc.spec.in ./sc.spec && perl -pi -e 's/\@VERSION@/2.2.11/' ./sc.spec

rpm :: srpm 
	rpmbuild -tb $(NAME)-$(VERSION).tar.gz

srpm :: rpm_prep manifest dist
	rpmbuild -ts $(NAME)-$(VERSION).tar.gz

manext_install ::
	cd docs && \
	install -d $(DESTDIR)$(INSTALLMAN1DIR)/../man5 && \
	$(POD2MAN5_EXE) systemconfig.conf.pod $(DESTDIR)$(INSTALLMAN1DIR)/../man5/systemconfig.conf.5 && \
	cd ..

set_ver::
	perl -pi -e 's/EXEVERSION =.*/EXEVERSION = "2.2.11";/' ./blib/script/systemconfigurator
	perl -pi -e 's/EXEVERSION =.*/EXEVERSION = "2.2.11";/' ./bin/systemconfigurator
	cp -a ./sc.spec.in ./sc.spec && perl -pi -e 's/\@VERSION@/2.2.11/' ./sc.spec

all :: set_ver manifest

install :: manext_install



# End.
