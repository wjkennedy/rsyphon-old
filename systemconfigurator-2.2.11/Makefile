	  lib/Boot/Aboot.pm blib/lib/Boot/Aboot.pm \
	  lib/Hardware/PCI/Detect.pm blib/lib/Hardware/PCI/Detect.pm \
	  lib/Time/Zone/Debian.pm blib/lib/Time/Zone/Debian.pm \
	  lib/Time.pm blib/lib/Time.pm \
	  lib/Keyboard.pm blib/lib/Keyboard.pm \
	  lib/Network.pm blib/lib/Network.pm \
	  lib/Modules/ConfModules.pm blib/lib/Modules/ConfModules.pm \
